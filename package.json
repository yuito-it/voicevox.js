{
  "name": "voicebox-api-client",
  "version": "0.1.5",
  "description": "A client for the Voicebox API, providing text-to-speech capabilities.",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunx tsc -p tsconfig.esm.json && bunx tsc -p tsconfig.cjs.json"
  },
  "keywords": [
    "voicebox",
    "api",
    "client",
    "speech",
    "text-to-speech",
    "tts"
  ],
  "author": "Yuito Akatsuki <yuito@yuito-it.jp>",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "axios": "^1.9.0"
  }
}
